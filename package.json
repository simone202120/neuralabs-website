{
  "name": "sigmalabs-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next",
    "clean:all": "rimraf .next node_modules/.cache",
    "kill": "powershell -ExecutionPolicy Bypass -File ./scripts/kill-dev-processes.ps1",
    "check": "powershell -ExecutionPolicy Bypass -File ./scripts/check-processes.ps1",
    "kill:ports": "FOR /F \"tokens=5\" %P IN ('netstat -ano ^| findstr :3000 :3001 :3002') DO taskkill /F /PID %P 2>nul || echo No processes on ports 3000-3002",
    "dev:clean": "npm run kill && npm run clean && npm run dev",
    "dev:safe": "npm run kill && npm run dev",
    "fresh": "npm run kill && npm run clean && npm run dev",
    "fresh:install": "npm run kill && npm run clean:all && npm install && npm run dev"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@portabletext/react": "^3.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@react-three/drei": "^9.112.0",
    "@react-three/fiber": "^8.16.8",
    "@sanity/image-url": "^1.1.0",
    "@sanity/types": "^3.62.2",
    "@sanity/vision": "^3.62.2",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.47.10",
    "@types/canvas-confetti": "^1.6.4",
    "autoprefixer": "^10.4.20",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.5.1",
    "framer-motion": "^11.11.17",
    "lucide-react": "^0.460.0",
    "next": "^14.2.15",
    "next-sanity": "^9.8.15",
    "next-themes": "^0.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "resend": "^4.0.1",
    "sanity": "^3.62.2",
    "tailwind-merge": "^2.5.5",
    "three": "^0.170.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/three": "^0.170.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.15",
    "postcss": "^8.4.49",
    "rimraf": "^6.1.2",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3"
  }
}
